          "type": "object",
          "required": [
            "ack_fee",
            "denom",
            "recv_fee",
            "timeout_fee"
          ],
          "properties": {
            "ack_fee": {
              "$ref": "#/definitions/Uint128"
            },
            "denom": {
              "type": "string"
            },
